# Apple Music API – Developer Token (JWT)
# From Apple Developer: Keys for your App, then create a key with Music Kit.
APPLE_TEAM_ID=
APPLE_KEY_ID=
APPLE_PRIVATE_KEY=

# setlist.fm API (optional; used by API proxy to hide key from client)
# https://api.setlist.fm/docs/1.0/index.html
SETLISTFM_API_KEY=

# Next.js / PWA – Apple Music app identifier (from Apple Developer)
NEXT_PUBLIC_APPLE_MUSIC_APP_ID=
